{
  "name": "kilvn/ripple-lib",
  "description": "A PHP API for interacting with Ripple RPC",
  "license": "MIT",
  "type": "library",
  "support": {
    "issues": "https://github.com/kilvn/ripple-lib/issues",
    "source": "https://github.com/kilvn/ripple-lib"
  },
  "authors": [
    {
      "name": "Shamsudin Serderov",
      "email": "steein.shamsudin@gmail.com"
    }
  ],
  "keywords": [
    "kilvn",
    "ripple",
    "ripple-lib",
    "ripple-php",
    "ripple-api",
    "ripple-rpc-api"
  ],
  "require": {
    "php": "^7.4",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/var-dumper": "^4.1|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Kilvn\\RippleAPI\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
